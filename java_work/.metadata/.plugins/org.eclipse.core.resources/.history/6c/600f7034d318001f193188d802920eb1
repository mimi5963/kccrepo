package kosa.mission;

public class Mission1 {
	public static void main(String[] args) {
		// 아이고 습관이 
		// 이름, 나이 , 주소 
		String name="장원";
		short age =28;
		String address="서울특별시 홍제동 158-29 301호";
		
		System.out.println(name);
		System.out.println(age);
		System.out.println(address);
		
		
	}
}
