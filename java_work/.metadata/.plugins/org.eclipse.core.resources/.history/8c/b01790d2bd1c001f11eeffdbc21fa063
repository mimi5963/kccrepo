package kcc.oop;

public class Account {
	
	//객체 생성을 위해 ! 
	
	String accountNo;
	String ownerName;
	int balance;
	
	public void deposit(int amount) {
		balance += amount;
	}
	//출금 
	public int withdraw(int amount) throws Exception{
		if(balance<amount) {
			throw new Exception("잔액부족");
		}
		
		balance -= amount;
		return amount;
	} 
	

}
