package kosa.basic;

import java.util.Scanner;

public class Loop {
	public static void main(String[] args) {

		int i=1;
		
		while(i<=9) {
			System.out.println("7 * "+i+" = "+(7*i));
			i++;
		}
		
		
		Scanner sc = new Scanner(System.in);
		int n =0;
		do {
			System.out.print("입력");
			n = sc.nextInt();
		}while(n<0);
		
		System.out.println("입력 결과: "+n);
		
	}
}
