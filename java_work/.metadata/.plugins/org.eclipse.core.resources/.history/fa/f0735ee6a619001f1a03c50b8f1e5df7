package kosa.mission;

public class ArrayMission {

	public static void main(String[] args) {
			
		
		int number = 5;
		
		// 1~5, 6~10 이런식 출력
		for(int i=0; i<number;i++) {
			for(int j=(i*number)+1; j<= (i+1)*number;j++) {
				if(j<=10) {
					System.out.printf("%3d",j);
				}else
				System.out.print(j+" ");
			}
			System.out.println();
		}
		
	}
}
