package kcc.cafe;

public class Customer {
	private String name;
	private Coupon coupon;
	
	
	public Customer() {}
	public Customer(String name) {
		this.name = name;
		this.coupon = createCoupone();
	}
	
	//랜덤 생성 
	public Coupon createCoupone() {
		//추후 랜덤으로 만들 것
		return new Coupon("총할인쿠폰",0.1);
	}
}
